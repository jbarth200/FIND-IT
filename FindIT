    global Searched_Service_Row_Location_IPX_Y_Device_Value


    hello3 = "Searching " + Service.get()
    myLabel3 = Label(root, text=hello3)
    myLabel3.grid(row=2, column=6)
    ##Indexing the location of the searched service to get Row#
    Searched_Service_Row_Location = excel_data_df_Sheet_1[excel_data_df_Sheet_1['ServiceName'] == Service.get().upper()].index
    print(Searched_Service_Row_Location)
    ##Grabbing the cell value from that indexed row and specific column, in this case pulling the value of indexed cell and column "Zixi MGMT"
    Searched_Service_Row_Location_Zixi_Device = (excel_data_df_Sheet_1.iloc[Searched_Service_Row_Location]["Zixi Device"])
    Searched_Service_Row_Location_Zixi_Device_Value = (Searched_Service_Row_Location_Zixi_Device.values)
