    Source6 = Label(Standard_window, text=Searched_Service_Row_Location_Gateway_Card_Multicast_Value)
    Source6.place(x=430, y=125)
    Source7 = Label(Standard_window, text=Searched_Service_Row_Location_IPX_Device_Value, fg="blue", cursor="hand2")
    Source7.bind("<Button-1>", lambda event: webbrowser.open(Source3.cget("text")))
    Source7.place(x=550, y=100)
    Source19 = Label(Standard_window, text=Searched_Service_Row_Location_IPX_Output_Port_Value)
    Source19.place(x=563, y=80)
    Source8 = Label(Standard_window, text=Searched_Service_Row_Location_IPX_Multicast_Value)
    Source8.place(x=685, y=125)
