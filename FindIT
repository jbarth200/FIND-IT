    Source10.place(x=310, y=60)
    Source11 = Label(Standard_window, text=Column_Name_AJ, font=('Helvetica', 11, 'bold'))
    Source11.place(x=575, y=60)
    Source12 = Label(Standard_window, text=Searched_Service_Row_Location_Gateway_Card_Source_Value)
    Source12.place(x=430, y=145)
    Source13 = Label(Standard_window, text=Searched_Service_Row_Location_Gateway_Card_Source_Value)
    Source13.place(x=685, y=145)

    my_canvas.create_line(40, 35, 40, 475, fill='black', dash=(3, 5))
    my_canvas.create_line(40, 35, 850, 35, fill='black', dash=(3, 5))
    my_canvas.create_line(850, 475, 850, 35, fill='black', dash=(3, 5))
    my_canvas.create_line(850, 35, 850, 35, fill='black', dash=(3, 5))
    my_canvas.create_line(850, 475, 40, 475, fill='black', dash=(3, 5))
