    if My_Service_List.__contains__(Service.get().upper()) and (Service.get().upper()) == "LX NATIONAL":
       return Standard_HUB_LX_Service_Search()

    elif My_Service_List.__contains__(Service.get().upper()) and (Service.get().upper()) == "PEACOCK EPL":
       return Standard_Stamford_Sports_Service_Search()

    elif My_Service_List.__contains__(Service.get().upper()):
       return Standard_Peacock_Service_Search()

    else:
        Not_Found = Tk()
        Not_Found.title("FINDIT")
        Not_Found.geometry('200x35')
        myButton_Not_Found = Label(Not_Found, text="Service Was Not Found", font=('Helvetica', 12, 'bold'))
        myButton_Not_Found.pack()

