###database = pd.ExcelFile('C:/Code/CODETEST.xlsx').parse(sheet_name="Sheet1", skiprows=0, nrows=3)

excel_data_df_Sheet_1 = pd.read_excel('CODETEST.xlsx', sheet_name='Sheet1', )

##Creating a service list for all services listed under "Serivce name" Column
My_Service_List = (excel_data_df_Sheet_1['ServiceName'].tolist())



##JUST CHANGED MY_MULTICAST_LIST!!!! 09/14
My_Multicast_List = (excel_data_df_Sheet_1[['Dry Creek TVE Encoder Multicast',"Dry Creek Contribution Encoder Multicast","10G WAN DCM Multicast","MUX Multicast","Zixi Multicast XA","Zixi Multicast XB","X NAT US West Card Multicast","X NAT US East Card Multicast","Y NAT US West Card Multicast","Y NAT US East Card Multicast","Gateway Card Multicast","IPX Multicast","HUB DCM X Multicast","HUB DCM Y Multicast","SPOKE DCM X Multicast","SPOKE DCM Y Multicast","NAT X EC CARD Multicast","NAT Y EC CARD Multicast","NAT X EAST CARD Multicast","NAT Y EAST CARD Multicast","Stamford Ya Gateway Multicast","Stamford Yb Gateway Multicast"]].values)
My_Multicast_List2 = (excel_data_df_Sheet_1['Gateway Card Multicast'].tolist())
My_Multicast_List3 = (excel_data_df_Sheet_1['IPX Multicast'].tolist())

column2 = excel_data_df_Sheet_1.iloc[0, 3]
column3 = excel_data_df_Sheet_1.iloc[:0, 4]

root = Tk()
