##Creating Signal Flow Box For Multicast Search
def Standard_Peacock_Multicast_Search():
    global Source1
    global My_Service_List
    global Test
    global column2
    global Searched_Service_Row_Location_Device
    global Searched_Service_Row_Location_Zixi_Device_Value
    global Searched_Service_Row_Location_Zixi_Multicast_XA_Value
    global Searched_Service_Row_Location_Zixi_Multicast_XB_Value
    global Searched_Service_Row_Location_Gateway_Device_Value
    global Searched_Service_Row_Location_Zixi_Source_Value
    global Searched_Service_Row_Location_Gateway_Card_Group_Service_Value
    global Searched_Service_Row_Location_Gateway_Card_Multicast_Value
    global Searched_Service_Row_Location_IPX_Device_Value
    global Searched_Service_Row_Location_IPX_Output_Port_Value
    global Searched_Service_Row_Location_IPX_Multicast_Value
    global Searched_Service_Row_Location_Gateway_Card_Source_Value
    global Searched_Service_Row_Location_B
    global Column_Name_O
    global Column_Name_AF
    global Searched_Multicast_Row_Location_B
    global Searched_Multicast_Row_Location_Zixi_Device_Value
    global Searched_Multicast_Row_Location_Zixi_Multicast_XA_Value
    global Searched_Multicast_Row_Location_Gateway_Device_Value
    global Searched_Multicast_Row_Location_Zixi_Source_Value
    global Searched_Multicast_Row_Location_Gateway_Card_Group_Service_Value
    global Searched_Multicast_Row_Location_Gateway_Card_Multicast_Value
    global Searched_Multicast_Row_Location_IPX_Device_Value
    global Searched_Multicast_Row_Location_IPX_Output_Port_Value
    global Searched_Multicast_Row_Location_IPX_Multicast_Value
    global Searched_Multicast_Row_Location_Gateway_Card_Source_Value
    global Searched_Multicast_Row_Location_Gateway_Card_Group_API_Value
    global Searched_Multicast_Row_Location_Gateway_Card_Service_API_Value
    global Searched_Multicast_Row_Location_Gateway_Card_MGMT_API_Value
    global group_a_stream_rate_in
    Standard_window = Tk()
    Standard_window.title("FINDIT")
    my_canvas = Canvas(Standard_window, width=1000, height=600, background='white')
    my_canvas.create_line(100, 110, 300, 110, fill='black', arrow=LAST)
    my_canvas.create_line(300, 110, 550, 110, fill='black', arrow=LAST)
    my_canvas.create_line(550, 110, 800, 110, fill='black', arrow=LAST)
    my_canvas.grid(row=3, column=2)

    def Gateway_Pull_Current_Rate_A():
        global Searched_Multicast_Row_Location_Gateway_Card_Group_API_Value
        global Searched_Multicast_Row_Location_Gateway_Card_Service_API_Value
        global Searched_Multicast_Row_Location_Gateway_Card_MGMT_API_Value
        global group_a_stream_rate_in
        print(Searched_Multicast_Row_Location_Gateway_Card_Group_API_Value)
        print(Searched_Multicast_Row_Location_Gateway_Card_Service_API_Value)
        print(Searched_Multicast_Row_Location_Gateway_Card_MGMT_API_Value)
        group_1_stream_rate_list = []
        tiii = []
        tiii.append(Searched_Multicast_Row_Location_Gateway_Card_Group_API_Value)

        tii = []
        tii.append(Searched_Multicast_Row_Location_Gateway_Card_Service_API_Value)
        user_input = []
        user_input.append(Searched_Multicast_Row_Location_Gateway_Card_MGMT_API_Value)

        tii = Searched_Multicast_Row_Location_Gateway_Card_Service_API_Value[0]
        tii = int(tii)

        tiii = Searched_Multicast_Row_Location_Gateway_Card_Group_API_Value[0]
        tiii = int(tiii)

        user_input = Searched_Multicast_Row_Location_Gateway_Card_MGMT_API_Value[0]

        group_a_stream_rate = requests.get(f"{user_input}//htdocs/v.api/apis/EV/GET/parameters/981.{tiii}.{tii}")

        group_a_stream_rate_return = group_a_stream_rate.json()

        group_a_stream_rate_in = group_a_stream_rate_return['value']

        Source2002 = Label(Standard_window, text=group_a_stream_rate_in, fg="blue", cursor="hand2")

        Source2002.place(x=210, y=250)


    if str(Searched_Multicast_Row_Location_Zixi_Device_Value) in (None, "[nan]"):
        pass
    else:
        Source1 = Label(Standard_window, text=Searched_Multicast_Row_Location_Zixi_Device_Value, fg="blue",
                        cursor="hand2")
        Source1.bind("<Button-1>", lambda event: webbrowser.open("https://nbcugmo.zen.zixi.com/#/clusters"))
        Source1.place(x=60, y=100)
        ##https://nbcugmo.zen.zixi.com/#/clusters/dc-peacock-lx-x-01
        ##Source1 = Label(Standard_window, text=Searched_Multicast_Row_Location_Zixi_Device_Value)
        ##Source1.place(x=60, y=100)
    if str(Searched_Multicast_Row_Location_Zixi_Multicast_XA_Value) in (None, "[nan]"):
        pass
    else:
        Source2 = Label(Standard_window, text=Searched_Multicast_Row_Location_Zixi_Multicast_XA_Value)
        Source2.place(x=200, y=125)
        ##Source15 = Label(Standard_window, text="XA", font=('Helvetica', 11, 'bold'))
        ##Source15.place(x=230, y=165)
    if str(Searched_Multicast_Row_Location_Zixi_Multicast_XB_Value) in (None, "[nan]"):
        pass
    else:
        Source2 = Label(Standard_window, text=Searched_Multicast_Row_Location_Zixi_Multicast_XB_Value)
        Source2.place(x=200, y=355)
        my_canvas.create_line(120, 120, 120, 400, fill='black')
        my_canvas.create_line(120, 400, 350, 400, fill='black')
        my_canvas.create_line(350, 400, 350, 125, fill='black', arrow=LAST)
    ## Source14 = Label(Standard_window, text="XB", font=('Helvetica', 11, 'bold'))
    ## Source14.place(x=230, y=335)
    if str(Searched_Multicast_Row_Location_Gateway_Device_Value) in (None, "[nan]"):
        pass
    else:
        Source3 = Label(Standard_window, text=Searched_Multicast_Row_Location_Gateway_Device_Value, fg="blue", cursor="hand2")
        Source2001 = Button(Standard_window, text="Stream Rate In", fg="blue", cursor="hand2", command=Gateway_Pull_Current_Rate_A)
        ##Source2002 = Label(Standard_window, text=group_1_stream_rate_in, fg="blue",cursor="hand2")
        Source3.bind("<Button-1>", lambda event: webbrowser.open(Source3.cget("text")))
        Source3.place(x=300, y=100)
        Source2001.place(x=200, y=220)

        ##Source2002.place(x=300, y=300)
    if str(Searched_Multicast_Row_Location_Zixi_Source_XA_Value) in (None, "[nan]"):
        pass
    else:
        Source4 = Label(Standard_window, text=Searched_Multicast_Row_Location_Zixi_Source_XA_Value)
        Source4.place(x=215, y=145)
    if str(Searched_Multicast_Row_Location_Zixi_Source_XB_Value) in (None, "[nan]"):
        pass
    else:
        Source4 = Label(Standard_window, text=Searched_Multicast_Row_Location_Zixi_Source_XB_Value)
        Source4.place(x=215, y=370)
    Source5 = Label(Standard_window, text=Searched_Multicast_Row_Location_Gateway_Card_Group_Service_Value)
    Source5.place(x=285, y=80)
    Source6 = Label(Standard_window, text=Searched_Multicast_Row_Location_Gateway_Card_Multicast_Value)
    Source6.place(x=430, y=125)
    Source7 = Label(Standard_window, text=Searched_Multicast_Row_Location_IPX_Device_Value, fg="blue", cursor="hand2")
    Source7.bind("<Button-1>", lambda event: webbrowser.open(Source3.cget("text")))
    Source7.place(x=550, y=100)
    Source19 = Label(Standard_window, text=Searched_Multicast_Row_Location_IPX_Output_Port_Value)
    Source19.place(x=563, y=80)
    Source8 = Label(Standard_window, text=Searched_Multicast_Row_Location_IPX_Multicast_Value)
    Source8.place(x=685, y=125)
    Source9 = Label(Standard_window, text=Column_Name_O, font=('Helvetica', 11, 'bold'))
    Source9.place(x=85, y=80)
    Source10 = Label(Standard_window, text=Column_Name_AF, font=('Helvetica', 11, 'bold'))
    Source10.place(x=310, y=60)
    Source11 = Label(Standard_window, text=Column_Name_AJ, font=('Helvetica', 11, 'bold'))
    Source11.place(x=575, y=60)
    Source12 = Label(Standard_window, text=Searched_Multicast_Row_Location_Gateway_Card_Source_Value)
    Source12.place(x=430, y=145)
    Source13 = Label(Standard_window, text=Searched_Multicast_Row_Location_Gateway_Card_Source_Value)
    Source13.place(x=685, y=145)

    my_canvas.create_line(40, 35, 40, 475, fill='black', dash=(3, 5))
    my_canvas.create_line(40, 35, 850, 35, fill='black', dash=(3, 5))
    my_canvas.create_line(850, 475, 850, 35, fill='black', dash=(3, 5))
    my_canvas.create_line(850, 35, 850, 35, fill='black', dash=(3, 5))
