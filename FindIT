        my_canvas.create_line(120, 400, 350, 400, fill='black')
        my_canvas.create_line(350, 400, 350, 125, fill='black', arrow=LAST)
       ## Source14 = Label(Standard_window, text="XB", font=('Helvetica', 11, 'bold'))
       ## Source14.place(x=230, y=335)
    if str(Searched_Service_Row_Location_Gateway_Device_Value) in (None, "[nan]"):
        pass
    else:
