##Creating Function For Source IP Search
def Okay2():
    hello2 = "Searching " + Source.get()
    myLabel2 = Label(root, text=hello2)
    myLabel2.grid(row=1, column=6)

##Creating Function For Service Search
def Service_Search():
    global Searched_Service_Row_Location_B
    global Searched_Service_Row_Location_Zixi_Device_Value
