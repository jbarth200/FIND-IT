    print(Searched_Service_Row_Location_Zixi_Multicast_XB_Value)
    Searched_Service_Row_Location_Gateway_Device = (excel_data_df_Sheet_1.iloc[Searched_Service_Row_Location]["Gateway"])
    Searched_Service_Row_Location_Gateway_Device_Value = (Searched_Service_Row_Location_Gateway_Device.values)
    print(Searched_Service_Row_Location_Gateway_Device_Value)
    Searched_Service_Row_Location_Zixi_Source_XA = (excel_data_df_Sheet_1.iloc[Searched_Service_Row_Location]["Zixi Source XA"])
    Searched_Service_Row_Location_Zixi_Source_XA_Value = (Searched_Service_Row_Location_Zixi_Source_XA.values)
    print(Searched_Service_Row_Location_Zixi_Source_XA_Value)
    Searched_Service_Row_Location_Zixi_Source_XB = (excel_data_df_Sheet_1.iloc[Searched_Service_Row_Location]["Zixi Source XB"])
    Searched_Service_Row_Location_Zixi_Source_XB_Value = (Searched_Service_Row_Location_Zixi_Source_XB.values)
