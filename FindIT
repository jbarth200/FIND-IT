        ##Source15 = Label(Standard_window, text="XA", font=('Helvetica', 11, 'bold'))
        ##Source15.place(x=230, y=165)
    if str(Searched_Service_Row_Location_Zixi_Multicast_XB_Value) in (None, "[nan]"):
        pass
    else:
